  "devDependencies": {
    "@defisaver/sdk": "0.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "chai": "^4.2.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.2",
    "husky": "^4.2.3",
    "mocha": "^7.1.2",
    "sol-merger": "^2.0.1",
    "solhint": "^3.2.2",
    "axios": "^0.21.1",
    "decimal.js": "^10.2.0",
    "dotenv": "^6.2.0"
